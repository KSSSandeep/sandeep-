 * @author Krishna Saidfgxdfgndrg
 */

public class Employee {
 
 private String id;
 private String name;
 private String dob;
 
 public Employee(){
     
 }
 public Employee(String id){
     this.id = id;
 }
 public Employee(String id,String name,String dob){
     this.id=id;
     this.name=name;
     this.dob=dob;

}

    

