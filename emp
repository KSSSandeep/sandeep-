package employee_gui;

/**
 *
 * @author Krishna Sai
 */

public class Employee {
 
 private String id;
 private String name;
 private String dob;
 
 public Employee(){
     
 }
 public Employee(String id){
     this.id = id;
 }
 public Employee(String id,String name,String dob){
     this.id=id;
     this.name=name;
     this.dob=dob;
 }
public String getid(){
    return this.id;
}
public String getname(){
    return this.name;
}   
public String getdob(){
    return this.dob;
}
}
    

